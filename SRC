class NumberThread extends Thread {

    public NumberThread(String name) {
        this.setName(name); 
    }

    @Override
    public void run() {
        for (int i = 1; i <= 10; i++) {
            System.out.println(getName() + ": " + i);
        }
    }
}

public class Main {
    public static void main(String[] args) {
        for (int i = 1; i <= 5; i++) {
            NumberThread t = new NumberThread("Thread-" + i);
            t.start();
        }
    }
}
